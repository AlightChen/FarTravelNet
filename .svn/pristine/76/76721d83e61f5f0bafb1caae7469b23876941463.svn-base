using SqlSugar;

namespace DAL.Entitys
{
    /// <summary>
    /// 用户表
    /// </summary>
    public class Userinfo
    {
        /// <summary>
        /// 用户表
        /// </summary>
        public Userinfo()
        {
        }

        private System.Int64 _ID;
        /// <summary>
        /// 用户ID
        /// </summary>
        [SugarColumn(IsPrimaryKey = true)]
        public System.Int64 ID { get { return this._ID; } set { this._ID = value; } }

        private System.String _USERCODE;
        /// <summary>
        /// 用户账号
        /// </summary>
        public System.String USERCODE { get { return this._USERCODE; } set { this._USERCODE = value?.Trim(); } }

        private System.String _USERNAME;
        /// <summary>
        /// 用户昵称
        /// </summary>
        public System.String USERNAME { get { return this._USERNAME; } set { this._USERNAME = value?.Trim(); } }

        private System.String _PASSWORD;
        /// <summary>
        /// 密码
        /// </summary>
        public System.String PASSWORD { get { return this._PASSWORD; } set { this._PASSWORD = value?.Trim(); } }

        private System.String _HEADPORTRAIT;
        /// <summary>
        /// 用户头像
        /// </summary>
        public System.String HEADPORTRAIT { get { return this._HEADPORTRAIT; } set { this._HEADPORTRAIT = value?.Trim(); } }

        private System.DateTime? _CREATETIME;
        /// <summary>
        /// 创建时间
        /// </summary>
        public System.DateTime? CREATETIME { get { return this._CREATETIME; } set { this._CREATETIME = value; } }

        private System.DateTime? _UPDATETIME;
        /// <summary>
        /// 更新时间
        /// </summary>
        public System.DateTime? UPDATETIME { get { return this._UPDATETIME; } set { this._UPDATETIME = value; } }

        private System.DateTime? _LASRLOGINTIME;
        /// <summary>
        /// 最后登入时间
        /// </summary>
        public System.DateTime? LASRLOGINTIME { get { return this._LASRLOGINTIME; } set { this._LASRLOGINTIME = value; } }

        private System.Boolean _ENABLE;
        /// <summary>
        /// 是否启用
        /// </summary>
        public System.Boolean ENABLE { get { return this._ENABLE; } set { this._ENABLE = value; } }

        private System.Boolean _UMDELETE;
        /// <summary>
        /// 标记删除
        /// </summary>
        public System.Boolean UMDELETE { get { return this._UMDELETE; } set { this._UMDELETE = value; } }
    }
}